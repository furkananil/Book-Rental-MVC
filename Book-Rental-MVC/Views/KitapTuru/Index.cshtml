@model List<KitapTuru>

<h1>Kitap Türleri</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Kitap Türü</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Ad</td>
            </tr>
        }
    </tbody>
</table>